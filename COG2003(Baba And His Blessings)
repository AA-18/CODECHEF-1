// https://www.codechef.com/COG2020/problems/COG2003/
// SOLUTION SOLVED IN C LANGUAGE

#include <stdio.h>

int main(void) {
	// your code goes here
	int T,N,y;
	y = - pow(10,9)
	scanf("%d",&T);
	int p[100000];
	while(T--)
	{
	    int i=0;
	    scanf("%d",&N);
	    int largest=y;
	    while(N--)
	    {
	        scanf("%d",&p[i]);
	        if (p[i]>largest)
	        {
	            largest=p[i];
	        }
	        i=i+1;
	    }
	    printf ("%d\n",largest);
	}
	
	return 0;
}

